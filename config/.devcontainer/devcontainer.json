{
  "name": "Go & PostgreSQL with Kafka and Redis",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "jetbrains": {
      "backend": "GoLand"
    }
  },
  "forwardPorts": [
    5432,
    9092
  ],
  "postCreateCommand": "go version",
  "remoteUser": "vscode"
}
